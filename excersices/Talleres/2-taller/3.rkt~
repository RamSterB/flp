(define (predicado-numero
          (lambda (func-pred n lst)
            (cond
              [(func-pred (car lst)) (predicado-numero func-pred (- n 1) (cdr lst))]
              [(= 0 n) #t]
              [(not (func-pred (car lst))) (predicado-numero func-pred n (cdr lst))]
              [else #f]
))))

(display (predicado-numero number? 5 '(0 1 1 2 "hola")))
